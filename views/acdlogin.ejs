    <section class="container" style="display: block; margin: auto; margin-top: 100px;">
        <h3 align="center" style="color: white;">Analog Circuit Design</h3>
		<form style="width: 100%;">
			<div class="form-group column large-8" style="margin:auto;">
				<label>Wissenaire ID </label>
				<input type="text" class="form-control" name="username" placeholder="Use the id used while registration" required>
			</div>
			<div class="form-group column large-8"  style="margin:auto;">
				<label>Mobile Number</label>
				<input type="password" class="form-control" name="password" placeholder="Use the mobile number used while registration" required>
			</div>

			
			<button type="submit" style="display:block; margin:auto;" id="submit" class="btn btn-dark">Proceed to Exam </button>
		  </form>
		  <script>
			$("form").submit(function(e) {
			  e.preventDefault(); 
			  var form = $(this);
			  $.ajax({
				  type: "POST",
				  url: '/acd/login',
				  data: form.serialize(), 
				  success: function(result)
				  {
					  if(result === 'Invalid Credentials'){
						  new swal({
							  title: 'Invalid Credentials',
							  text: result,
							  icon: 'error'
						  });
					  }
					  else if (result == 'You are all set'){
						  new swal({
							  title: 'All the Best !',
							  text: result,
							  icon: 'success'
						  }).then((value)=>{
							 window.location.href = '/acd/rules';
						  })
					  }
					  else if(result == 'Already Logged in'){
						new swal({
							  title: 'Already Logged in',
							  text: result,
							  icon: 'error'
						  });
					  }
				  }
				  });
			  });
		</script>
